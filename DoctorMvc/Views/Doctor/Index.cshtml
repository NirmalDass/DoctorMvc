                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Qualification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dept_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MobileNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AvailableDays)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.DoctorId }) |
                @*@Html.ActionLink("DetailsId", "DetailsId", new { id=item.DoctorId}) |*@
